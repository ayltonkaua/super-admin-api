    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@3.0.0"
        }
    }
}